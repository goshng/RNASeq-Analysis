blastp -query all.pep -db all.pep -evalue 1e-3 -outfmt 7 -out all.blastp.tab
mcxdeblast --m9 --score=e all.blastp.tab
mclblastline --mcl-I=1.6 --start-assemble all.blastp.tab
----
