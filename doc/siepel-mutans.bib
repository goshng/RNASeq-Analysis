  pmid = {21422186},
  review = {OMZ175},
  timestamp = {2011.06.16},
  url = {http://dx.doi.org/10.1128/IAI.00767-10}
}

@ARTICLE{Abranches2008,
  author = {Jacqueline Abranches and Marcelle M Nascimento and Lin Zeng and Christopher
